@font-face {
  font-family: "Nimbus Sans";
  src: url('{{"NimbusSanL-Reg.ttf" | | asset_url }}') format("truetype"),
  url('{{"NimbusSanL-Reg.woff" | | asset_url }}') format("woff"),
  url('{{"NimbusSanL-Reg.woff2" | | asset_url }}') format("woff2");
  font-weight: 400;
  font-style: normal;
}


/* DEFAULT STYLES */

* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

body {
  position: relative;
  background-color: #FFFFFF;
}

main {
  min-height: 100vh;
}

h1, h2, h3, h4, h5, h6, p,
span, li, a, input, button {
  font-family: "Nimbus Sans", Arial, sans-serif;
  font-style: normal;
  font-weight: 400;
  line-height: 1.2;
  text-decoration: none;
  color: #0F0F0F;
}

input, button {
  border: none;
  outline: none;
  background-color: transparent;
}

input::placeholder {
  color: #bdbdbd;
}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

input[type=number] {
  -moz-appearance: textfield;
}

button:hover, a:hover {
  cursor: pointer;
}





