/* CHECKBOX */

.checkbox {
  position: relative;
  display: flex;
  flex-direction: row;
}

.checkbox_form {
  margin-top: 16px;
  margin-bottom: 64px;
}

.checkbox_order {
  margin-bottom: 24px;
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.checkbox__true-checkbox {
  position: absolute;
  opacity: 0;
}

.checkbox__fake-checkbox {
  position: relative;
  display: flex;
  width: 25px;
  height: 25px;
  background-image: url({{ 'checkbox-cirlce.jpg' | asset_img_url: 'master' }});
  background-position: center;
  background-size: cover;
}

.checkbox__fake-checkbox:hover {
  cursor: pointer;
}

.checkbox__fake-checkbox::before {
  position: absolute;
  display: flex;
  content: "";
  width: 25px;
  height: 25px;
  background-image: url({{ 'checkbox-cross.jpg' | asset_img_url: 'master' }});
  background-position: center;
  background-size: cover;
  opacity: 0;
}

.checkbox__true-checkbox:checked +
.checkbox__fake-checkbox::before {
  opacity: 1;
}

.checkbox__description {
  margin-left: 24px;
  font-size: 14px;
}





/* FILTERS */

.filters {
