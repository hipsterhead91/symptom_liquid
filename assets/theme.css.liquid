  left: 0;
  opacity: 0;
}

.filters__fake-radio {
  width: 100%;
  font-size: 16px;
  line-height: 1;
  text-transform: uppercase;
}

.filters__fake-radio:hover {
  cursor: pointer;
}

 .filters__fake-radio::before {
  position: absolute;
  top: -12px;
  left: -28px;
  width: calc(100% + 56px);
  height: 38px;
  content: "";
  opacity: 0;
  border: 1px solid #0f0f0f;
  border-radius: 50%;
  /* pointer-events: unset; */
}

